{
    "name": "sahilr2050/omnipay-payway",
    "type": "library",
    "description": "PayWay REST API gateway for Omnipay payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "payway",
        "westpac",
        "purchase",
        "token"
    ],
    "homepage": "https://github.com/sahilr2050/omnipay-payway",
    "license": "MIT",
    "authors": [
        {
            "name": "Sahil Patel",
            "email": "admin@seecoding.com"
        }
    ],
    "config": {
        "bin-dir": "bin/",
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\PaywayRest\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0",
        "omnipay/common": "^3.3",
        "symfony/http-client": "^6.4",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "omnipay/tests": "^4.1",
        "http-interop/http-factory-guzzle": "^1.2"
    }
}
